          <DS_SQL><![CDATA[/*QUEBRA ESPECILIADADE PADRÃO*/
SELECT
distinct t.cd_especialidade CD_ESPECIALIDADE2,
t.ds_especialidade,
t.cd_procedimento procedimento2
FROM
(
SELECT
distinct obter_especialidade_medico(a.cd_medico_cirurgiao,'C')CD_ESPECIALIDADE,
c.cd_procedimento,
obter_especialidade_medico(a.cd_medico_cirurgiao,'D')DS_ESPECIALIDADE,
a.CD_TIPO_CIRURGIA,
a.IE_PORTE
FROM
    cirurgia a, HCB_CIRURGIA_SEGURA_V b, PROCEDIMENTO C
WHERE      
a.dt_inicio_real  between  :DT_INICIAL and FIM_DIA(:DT_FINAL) 
and c.CD_PROCEDIMENTO = a.CD_PROCEDIMENTO_PRINC
and a.nr_cirurgia = b.nr_cirurgia
AND a.CD_SETOR_ATENDIMENTO = :CD_SETOR_ATENDIMENTO 
and a.dt_termino is not null
and   a.ie_status_cirurgia in(2) 
and b.DS_ANTIB_SIM_AIC is not null ---NÃO PODE ESTA NULO
and :CD_ANTIBIOTICO = 1 --SIM ANTIBIOTICO
and :CD_PADRAO = 1 --PADRÃO CCIH
AND obter_especialidade_medico(a.cd_medico_cirurgiao,'C') IS NOT NULL
union
SELECT
distinct obter_especialidade_medico(a.cd_medico_cirurgiao,'C')CD_ESPECIALIDADE,
c.cd_procedimento,
obter_especialidade_medico(a.cd_medico_cirurgiao,'D')DS_ESPECIALIDADE,
a.CD_TIPO_CIRURGIA,
a.IE_PORTE
FROM
    cirurgia a, HCB_CIRURGIA_SEGURA_V b, PROCEDIMENTO C
WHERE      
a.dt_inicio_real  between  :DT_INICIAL and FIM_DIA(:DT_FINAL) 
and c.CD_PROCEDIMENTO = a.CD_PROCEDIMENTO_PRINC
and a.nr_cirurgia = b.nr_cirurgia
AND a.CD_SETOR_ATENDIMENTO = :CD_SETOR_ATENDIMENTO 
and a.dt_termino is not null
and   a.ie_status_cirurgia in(2) 
and b.DS_ANTIB_SIM_AIC is null -- ANTIBIÓTICO DEVE ESTAR NULLO
and :CD_ANTIBIOTICO = 0 --NÃO ANTIBIÓTICO
and :CD_PADRAO = 1 --PADRÃO CCIH
AND obter_especialidade_medico(a.cd_medico_cirurgiao,'C') IS NOT NULL
union
SELECT
distinct obter_especialidade_medico(a.cd_medico_cirurgiao,'C')CD_ESPECIALIDADE,
c.cd_procedimento,
obter_especialidade_medico(a.cd_medico_cirurgiao,'D')DS_ESPECIALIDADE,
a.CD_TIPO_CIRURGIA,
a.IE_PORTE
FROM
    cirurgia a, HCB_CIRURGIA_SEGURA_V b, PROCEDIMENTO C
WHERE      
a.dt_inicio_real  between  :DT_INICIAL and FIM_DIA(:DT_FINAL) 
and c.CD_PROCEDIMENTO = a.CD_PROCEDIMENTO_PRINC
and a.nr_cirurgia = b.nr_cirurgia
AND a.CD_SETOR_ATENDIMENTO = :CD_SETOR_ATENDIMENTO 
and a.dt_termino is not null
and   a.ie_status_cirurgia in(2) 
and :CD_ANTIBIOTICO = 2 --AMBOS
and :CD_PADRAO = 1  ----PADRÃO CCIH
AND obter_especialidade_medico(a.cd_medico_cirurgiao,'C') IS NOT NULL
) T
WHERE
1=1
AND t.cd_procedimento in (411010034,411010042,411010026,45080224,45080020,45080194,45080020,45080194,35009012,45080038,45080038,31309208,35084014,35082011,35085010,35028017,31309054,31309054,408040041,408040050,39003124,30724082,30724082,30724058,30724058,408040068,39018121,39016129,408040084,408040092,30719020,408050047,52130045,52130045,408050055, 30726034,30726034,39022145,408050063,408050071,49010140,49010140,403010101,96010099,96010107,96010038,96010093,30602351,96010064,96010058,96010045,96010082,96010061, 96010062,45090289,47010282,45090289,47010282,38059061,413040089,96010121,96010090,96010108)
@SQL_WHERE
order by 2]]></DS_SQL>
