        <registro DS_COR_FOOTER="clWhite" DS_COR_FUNDO="clWhite" DS_COR_HEADER="clSilver" DS_COR_QUEBRA="$00E5E5E5" DS_OBSERVACAO="" DT_ATUALIZACAO="22/08/2023 13:33:38" IE_ALTERNA_COR_FUNDO="S" IE_BANDA_PADRAO="N" IE_BORDA_DIR="N" IE_BORDA_ESQ="N" IE_BORDA_INF="N" IE_BORDA_SUP="N" IE_DIRECTION="" IE_FUNDO_TRANSPARENTE="" IE_IMPRIME_PRIMEIRO="X" IE_IMPRIME_VAZIO="N" IE_QUEBRA_PAGINA="N" IE_REIMPRIME_NOVA_PAGINA="N" IE_SITUACAO="" IE_TIPO_BANDA="S" NM_TABELA="" NM_USUARIO="EDUARDOK" NR_SEQ_APRESENTACAO="10" NR_SEQ_BANDA_SUPERIOR="6644073" NR_SEQ_RELATORIO="2856915" NR_SEQUENCIA="6644075" QT_ALTURA="18" QT_MAX_REGISTRO="" >
          <DS_SQL><![CDATA[SELECT
y.nr_cirurgia,y.ds_checkbox_26,y.DS_ANTIB_HORA_26,y.ds_checkbox_29,y.DS_ANT_HORA,y.ds_status,y.ds_porte,y.ds_procedimento,y.nr_atendimento,y.dt_termino,y.nm_paciente,y.telefone,y.nm_medico
FROM
(SELECT DISTINCT a.nr_cirurgia,DECODE(b.ds_antib_sim_aic,'','Não','X','Sim') ds_checkbox_26,b.DS_ANTIB_HORA DS_ANTIB_HORA_26,substr(obter_status_cirurgia(a.ie_status_cirurgia),1,200) ds_status,DECODE(a.ie_porte,'M','Médio','P','Pequeno','G','Grande','E','Especial','D','Superior','','Não Informado') ds_porte,c.ds_procedimento,a.nr_atendimento,a.dt_termino,obter_nome_pf(a.cd_pessoa_fisica) nm_paciente,obter_telefone_pf(a.cd_pessoa_fisica,12) telefone,obter_nome_pf(a.cd_medico_cirurgiao) nm_medico,a.cd_tipo_cirurgia,a.ie_porte,DECODE(b.ANT_S,'','Não','X','Sim') ds_checkbox_29,b.DS_ANT_HORA,obter_especialidade_medico(a.cd_medico_cirurgiao,'C') cd_especialidade
FROM cirurgia a,hcb_cirurgia_segura_v b,procedimento c
WHERE a.dt_inicio_real BETWEEN :dt_inicial and :dt_final AND c.cd_procedimento = a.cd_procedimento_princ AND a.nr_cirurgia = b.nr_cirurgia AND obter_especialidade_medico(a.cd_medico_cirurgiao,'C') =:cd_especialidade2 AND a.cd_setor_atendimento =:cd_setor_atendimento AND a.dt_termino IS NOT NULL AND a.ie_status_cirurgia IN (2) AND b.ds_antib_sim_aic IS NOT NULL and b.ANT_S is not null AND :cd_antibiotico = 1 AND :cd_padrao = 0
UNION
SELECT DISTINCT a.nr_cirurgia,DECODE(b.ds_antib_sim_aic,'','Não','X','Sim') ds_checkbox_26,b.DS_ANTIB_HORA DS_ANTIB_HORA_26,substr(obter_status_cirurgia(a.ie_status_cirurgia),1,200) ds_status,DECODE(a.ie_porte,'M','Médio','P','Pequeno','G','Grande','E','Especial','D','Superior','','Não Informado') ds_porte,c.ds_procedimento,a.nr_atendimento,a.dt_termino,obter_nome_pf(a.cd_pessoa_fisica) nm_paciente,obter_telefone_pf(a.cd_pessoa_fisica,12) telefone,obter_nome_pf(a.cd_medico_cirurgiao) nm_medico,a.cd_tipo_cirurgia,a.ie_porte,DECODE(b.ANT_S,'','Não','X','Sim') ds_checkbox_29,b.DS_ANT_HORA,obter_especialidade_medico(a.cd_medico_cirurgiao,'C') cd_especialidade
FROM cirurgia a,hcb_cirurgia_segura_v b,procedimento c
WHERE a.dt_inicio_real BETWEEN :dt_inicial and :dt_final AND c.cd_procedimento = a.cd_procedimento_princ AND a.nr_cirurgia = b.nr_cirurgia AND obter_especialidade_medico(a.cd_medico_cirurgiao,'C') =:cd_especialidade2 AND a.cd_setor_atendimento =:cd_setor_atendimento AND a.dt_termino IS NOT NULL AND a.ie_status_cirurgia IN (2) AND b.ds_antib_sim_aic IS NULL and b.ANT_S is null AND :cd_antibiotico = 0 AND :cd_padrao = 0
UNION
SELECT DISTINCT a.nr_cirurgia,DECODE(b.ds_antib_sim_aic,'','Não','X','Sim') ds_checkbox_26,b.DS_ANTIB_HORA DS_ANTIB_HORA_26,substr(obter_status_cirurgia(a.ie_status_cirurgia),1,200) ds_status,DECODE(a.ie_porte,'M','Médio','P','Pequeno','G','Grande','E','Especial','D','Superior','','Não Informado') ds_porte,c.ds_procedimento,a.nr_atendimento,a.dt_termino,obter_nome_pf(a.cd_pessoa_fisica) nm_paciente,obter_telefone_pf(a.cd_pessoa_fisica,12) telefone,obter_nome_pf(a.cd_medico_cirurgiao) nm_medico,a.cd_tipo_cirurgia,a.ie_porte,DECODE(b.ANT_S,'','Não','X','Sim') ds_checkbox_29,b.DS_ANT_HORA,obter_especialidade_medico(a.cd_medico_cirurgiao,'C') cd_especialidade
FROM cirurgia a,hcb_cirurgia_segura_v b,procedimento c
WHERE
    a.dt_inicio_real BETWEEN :dt_inicial AND fim_dia(:dt_final) AND c.cd_procedimento = a.cd_procedimento_princ AND a.nr_cirurgia = b.nr_cirurgia AND obter_especialidade_medico(a.cd_medico_cirurgiao,'C') =:cd_especialidade2 AND a.cd_setor_atendimento =:cd_setor_atendimento AND a.dt_termino IS NOT NULL AND a.ie_status_cirurgia IN (2) AND :cd_antibiotico = 2 AND :cd_padrao = 0
) y
WHERE 1 = 1
@sql_where]]></DS_SQL>
