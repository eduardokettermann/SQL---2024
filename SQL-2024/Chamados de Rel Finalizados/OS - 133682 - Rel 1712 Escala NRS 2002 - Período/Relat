          <DS_SQL><![CDATA[/*DADOS PADRÃO*/
select 
y.nr_cirurgia,
y.ds_checkbox,
y.ds_status,
DECODE(y.IE_PORTE,'M', 'Médio','P','Pequeno','G','Grande','E','Especial','D','Superior','','Não Informado') DS_PORTE,
y.ds_procedimento,
y.nr_atendimento,
y.dt_termino,
y.nm_paciente,
y.telefone,
y.nm_medico,
y.cd_procedimento 
from
(
SELECT
distinct a.nr_cirurgia,
c.cd_procedimento,
DECODE(b.DS_ANTIB_SIM_AIC,'','Não','X','Sim') DS_CHECKBOX,
substr(obter_status_cirurgia(A.IE_STATUS_CIRURGIA),1,200) DS_STATUS,
A.IE_PORTE,
C.DS_PROCEDIMENTO,
A.NR_ATENDIMENTO,
a.dt_termino,
OBTER_NOME_PF(a.cd_pessoa_fisica)NM_PACIENTE,
Obter_Telefone_PF(a.cd_pessoa_fisica,12) TELEFONE,
OBTER_NOME_PF(a.cd_medico_cirurgiao) NM_MEDICO,
a.CD_TIPO_CIRURGIA,
obter_especialidade_medico(a.cd_medico_cirurgiao,'C') CD_ESPECIALIDADE
FROM
    cirurgia a, HCB_CIRURGIA_SEGURA_V b, PROCEDIMENTO C
WHERE      
a.dt_inicio_real  between  :DT_INICIAL and FIM_DIA(:DT_FINAL) 
and c.CD_PROCEDIMENTO = a.CD_PROCEDIMENTO_PRINC
and a.nr_cirurgia = b.nr_cirurgia
AND a.CD_SETOR_ATENDIMENTO = :CD_SETOR_ATENDIMENTO 
and a.dt_termino is not null
and   a.ie_status_cirurgia in(2) 
and b.DS_ANTIB_SIM_AIC is not null
and :CD_ANTIBIOTICO = 1
and :CD_PADRAO = 1
and obter_especialidade_medico(a.cd_medico_cirurgiao,'C')=:CD_ESPECIALIDADE2
union
SELECT
distinct a.nr_cirurgia,
c.cd_procedimento,
DECODE(b.DS_ANTIB_SIM_AIC,'','Não','X','Sim') DS_CHECKBOX,
substr(obter_status_cirurgia(A.IE_STATUS_CIRURGIA),1,200) DS_STATUS,
A.IE_PORTE,
C.DS_PROCEDIMENTO,
A.NR_ATENDIMENTO,
a.dt_termino,
OBTER_NOME_PF(a.cd_pessoa_fisica)NM_PACIENTE,
Obter_Telefone_PF(a.cd_pessoa_fisica,12) TELEFONE,
OBTER_NOME_PF(a.cd_medico_cirurgiao) NM_MEDICO,
a.CD_TIPO_CIRURGIA,
obter_especialidade_medico(a.cd_medico_cirurgiao,'C') CD_ESPECIALIDADE
FROM
    cirurgia a, HCB_CIRURGIA_SEGURA_V b, PROCEDIMENTO C
WHERE      
a.dt_inicio_real  between  :DT_INICIAL and FIM_DIA(:DT_FINAL) 
and c.CD_PROCEDIMENTO = a.CD_PROCEDIMENTO_PRINC
and a.nr_cirurgia = b.nr_cirurgia
AND a.CD_SETOR_ATENDIMENTO = :CD_SETOR_ATENDIMENTO 
and a.dt_termino is not null
and   a.ie_status_cirurgia in(2) 
and b.DS_ANTIB_SIM_AIC is null
and :CD_ANTIBIOTICO = 0
and :CD_PADRAO = 1
and obter_especialidade_medico(a.cd_medico_cirurgiao,'C')=:CD_ESPECIALIDADE2
union
SELECT
distinct a.nr_cirurgia,
c.cd_procedimento,
DECODE(b.DS_ANTIB_SIM_AIC,'','Não','X','Sim') DS_CHECKBOX,
substr(obter_status_cirurgia(A.IE_STATUS_CIRURGIA),1,200) DS_STATUS,
A.IE_PORTE,
C.DS_PROCEDIMENTO,
A.NR_ATENDIMENTO,
a.dt_termino,
OBTER_NOME_PF(a.cd_pessoa_fisica)NM_PACIENTE,
Obter_Telefone_PF(a.cd_pessoa_fisica,12) TELEFONE,
OBTER_NOME_PF(a.cd_medico_cirurgiao) NM_MEDICO,
a.CD_TIPO_CIRURGIA,
obter_especialidade_medico(a.cd_medico_cirurgiao,'C') CD_ESPECIALIDADE
FROM
    cirurgia a, HCB_CIRURGIA_SEGURA_V b, PROCEDIMENTO C
WHERE      
a.dt_inicio_real  between  :DT_INICIAL and FIM_DIA(:DT_FINAL) 
and c.CD_PROCEDIMENTO = a.CD_PROCEDIMENTO_PRINC
and a.nr_cirurgia = b.nr_cirurgia
AND a.CD_SETOR_ATENDIMENTO = :CD_SETOR_ATENDIMENTO 
and a.dt_termino is not null
and   a.ie_status_cirurgia in(2) 
and :CD_ANTIBIOTICO = 2
and :CD_PADRAO = 1
and obter_especialidade_medico(a.cd_medico_cirurgiao,'C')=:CD_ESPECIALIDADE2
)Y
WHERE
1=1
AND y.cd_procedimento  =:procedimento2
@SQL_WHERE]]></DS_SQL>
