        <registro DS_COR_FOOTER="clWhite" DS_COR_FUNDO="clWhite" DS_COR_HEADER="clSilver" DS_COR_QUEBRA="clWhite" DS_OBSERVACAO="" DT_ATUALIZACAO="01/07/2020 08:53:37" IE_ALTERNA_COR_FUNDO="S" IE_BANDA_PADRAO="N" IE_BORDA_DIR="N" IE_BORDA_ESQ="N" IE_BORDA_INF="N" IE_BORDA_SUP="N" IE_DIRECTION="" IE_FUNDO_TRANSPARENTE="" IE_IMPRIME_PRIMEIRO="X" IE_IMPRIME_VAZIO="N" IE_QUEBRA_PAGINA="N" IE_REIMPRIME_NOVA_PAGINA="N" IE_SITUACAO="" IE_TIPO_BANDA="S" NM_TABELA="" NM_USUARIO="JOSEO" NR_SEQ_APRESENTACAO="12" NR_SEQ_BANDA_SUPERIOR="" NR_SEQ_RELATORIO="2848444" NR_SEQUENCIA="6558345" QT_ALTURA="17" QT_MAX_REGISTRO="" >
          <DS_SQL><![CDATA[select 
X.NR_ATENDIMENTO NR_ATENDIMENTO,
X.tipo_evolucao,
X.vl_totaL 
from
(
select
distinct nvl(SUBSTR(a.NR_ATENDIMENTO,1,255),'Pront:'||Obter_Prontuario_Paciente(A.CD_PESSOA_FISICA))NR_ATENDIMENTO,
a.IE_EVOLUCAO_CLINICA CD_EVOLUCAO_CLINICA,
Obter_Convenio_Atendimento(a.NR_ATENDIMENTO)cd_convenio,
b.DS_TIPO_EVOLUCAO tipo_evolucao,
count(a.IE_EVOLUCAO_CLINICA) vl_totaL
from TIPO_EVOLUCAO b, evolucao_paciente_lib_v a
where 
b.CD_TIPO_EVOLUCAO = a.IE_EVOLUCAO_CLINICA
AND A.IE_SITUACAO = 'A'
and a.DT_LIBERACAO  between  :DT_INICIAL and FIM_DIA(:DT_FINAL) 

group by a.IE_EVOLUCAO_CLINICA, Obter_Convenio_Atendimento(a.NR_ATENDIMENTO), nvl(SUBSTR(a.NR_ATENDIMENTO,1,255),'Pront:'||Obter_Prontuario_Paciente(A.CD_PESSOA_FISICA)), 
b.DS_TIPO_EVOLUCAO


) x
where 1 = 1
@SQL_WHERE
and :tipo_relatorio = 'S'
ORDER BY X.NR_ATENDIMENTO]]></DS_SQL>
>
