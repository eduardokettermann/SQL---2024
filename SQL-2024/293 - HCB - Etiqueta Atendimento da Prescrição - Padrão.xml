          <DS_SQL><![CDATA[select 'Turno: [ ] Manhã   [ ] Tarde   [ ] Noite  [ ] SN' ds_turno,
    'Paciente: ' || sem_acento (b.nm_paciente) nm_paciente,
    'Prescrição: ' || a.nr_prescricao nr_prescricao,
    'Atend.: ' || a.nr_atendimento nr_atendimento,
    'Dt Prescr.: ' || to_char(a.dt_prescricao, 'dd/mm/yyyy hh24:mm') dt_prescricao,
    'Setor: ' || b.ds_setor_atendimento ds_setor_atendimento,
    '1º Horário: ' || to_char(a.dt_primeiro_horario, 'hh:mm') dt_primeiro_horario,
    'Convênio: ' || b.ds_convenio ds_convenio,
    'Médico: ' || substr(obter_nome_medico(a.cd_medico, 'N'),1,40) nm_medico,
    'Dt Nasc: ' || to_char(b.dt_nascimento, 'dd/mm/yyyy') dt_nascimento,
    'Leito: ' || b.cd_unidade cd_unidade,
    a.nr_prescricao nr_presc_barras
from    atendimento_paciente_v b,
    prescr_medica a
where    a.nr_atendimento = b.nr_atendimento
and    a.nr_prescricao = :nr_prescricao]]></DS_SQL>
