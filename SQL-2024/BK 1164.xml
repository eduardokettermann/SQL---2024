        <registro CD_CGC_CLIENTE="87768735000148" CD_CLASSIF_RELAT="CATE" CD_CLASSIF_RELAT_PAIS="" CD_CLASSIF_RELAT_WHEB="" CD_PAIS="" CD_RELATORIO="1164" CD_RELATORIO_PAIS="" CD_RELATORIO_WHEB="" CD_VERSION="19413" DS_ACTION_NAME="" DS_COR_FUNDO="clWhite" DS_PROCEDURE="" DS_REPORT_TITLE_CUSTOM="" DT_ATUALIZACAO="20/12/2019 11:49:18" DT_LAST_MODIFICATION="20/12/2019 11:49:18" IE_AJUSTAR_TAMANHO="" IE_AUTO_AJUSTE="" IE_BORDA_DIR="S" IE_BORDA_ESQ="S" IE_BORDA_INF="S" IE_BORDA_SUP="S" IE_CHAMADA_DIRETA="" IE_ETIQUETA="N" IE_FILTRO_EXCEL="S" IE_FILTRO_HTML="S" IE_FILTRO_TEXTO="S" IE_FILTRO_WORD="S" IE_GERAR_BASE="S" IE_GERAR_RELATORIO="S" IE_IMPRIME_VAZIO="S" IE_ORIENTACAO="P" IE_TIPO_PAPEL="A4" IE_TIPO_RELATORIO="" NM_TABELA="" NM_USUARIO="EDSON" NR_SEQ_MOD_IMPL="" NR_SEQ_MODULO="123" NR_SEQ_ORDEM_SERV="" NR_SEQUENCIA="2847574" NR_VERSAO="" QT_ALTURA="" QT_COLUNA="1" QT_ESPACO_COLUNA="0" QT_LARGURA="" QT_MARGEM_DIR="20" QT_MARGEM_ESQ="7" QT_MARGEM_INF="10" QT_MARGEM_SUP="10" >
          <DS_SQL><![CDATA[SELECT 
	a.cd_material,
	SUBSTR(obter_desc_material(a.cd_material),1,120) ds_material,
	SUBSTR(DECODE(a.ie_acao,'I',obter_texto_tasy(458780, wheb_usuario_pck.get_nr_seq_idioma), obter_texto_tasy(458781, wheb_usuario_pck.get_nr_seq_idioma)),1,50) ie_acao,
	a.qt_material
FROM          mat_atend_pac_log a,cirurgia b
WHERE		   a.nr_prescricao = b.nr_prescricao
AND		   a.cd_funcao	IN	(900,-2009)
and a.DT_ENTRADA_UNIDADE is null
and b.dt_inicio_real	BETWEEN  inicio_dia(:dt_inicial ) AND fim_dia(:dt_final)
and	((a.ie_acao	= :ie_acao)  or  ( :ie_acao  = 'T'))
order by 2]]></DS_SQL>
>
